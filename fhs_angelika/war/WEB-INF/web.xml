<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
  
  <!-- Servlets -->
  <servlet>
      <servlet-name>mysqlService</servlet-name>
      <servlet-class>org.irri.households.server.MySQLServiceImpl</servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>mysqlService</servlet-name>
      <url-pattern>/fhs_angelika/mysqlservice</url-pattern>
  </servlet-mapping>
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>Fhs_angelika.html</welcome-file>
  </welcome-file-list>

</web-app>
